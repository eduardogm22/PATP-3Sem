<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PreviewBagController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <HBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="0.0" prefWidth="0.0" spacing="90.0" VBox.vgrow="ALWAYS">
                     <children>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" HBox.hgrow="ALWAYS">
                           <children>
                              <HBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="0.0" spacing="30.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#abrirCadastro" text="Registrar" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <TableView fx:id="tabelaPatrimonios" minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
                                <columns>
                                  <TableColumn fx:id="colID" text="ID" />
                                  <TableColumn fx:id="colNomePrd" text="Nome Prod." />
                                  <TableColumn fx:id="colCategoria" text="Categoria" />
                                  <TableColumn fx:id="colSetResp" text="Setor Resp." />
                                  <TableColumn fx:id="colSituacao" text="Situação" />
                                  <TableColumn fx:id="colValUn" text="Valor Un." />
                                  <TableColumn fx:id="colQuantidade" text="Quantidade" />
                                  <TableColumn fx:id="colRecebPor" text="Receb. por" />
                                  <TableColumn fx:id="colDtReceb" text="Dt Recebimento" />
                                  <TableColumn fx:id="colFornec" text="Fornecedor" />
                                  <TableColumn fx:id="colDtAquisicao" text="Dt Aquisição" />
                                  <TableColumn fx:id="colNumero" text="Número" />
                                  <TableColumn fx:id="colSerie" text="Série" />
                                </columns>
                              </TableView>
                           </children>
                           <padding>
                              <Insets bottom="15.0" left="90.0" right="15.0" top="15.0" />
                           </padding>
                        </VBox>
                        <VBox alignment="CENTER" spacing="45.0" HBox.hgrow="ALWAYS">
                           <children>
                               <Button mnemonicParsing="false" onAction="#editarItem" text="Editar" HBox.hgrow="ALWAYS" />
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="33.0" top="30.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</StackPane>
